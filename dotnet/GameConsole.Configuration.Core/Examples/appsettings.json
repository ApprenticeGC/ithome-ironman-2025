{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "GameConsole": "Debug"
    }
  },
  "GameConsole": {
    "Engine": {
      "Type": "Unity",
      "TargetFrameRate": 60
    },
    "Audio": {
      "Provider": "OpenAL",
      "Volume": 0.8,
      "EnableSpatial": true
    },
    "Input": {
      "Provider": "Direct",
      "EnableGamepad": true,
      "KeyboardLayout": "QWERTY"
    },
    "Configuration": {
      "HotReloadEnabled": true,
      "ValidationLevel": "Strict",
      "CacheEnabled": true
    }
  },
  "ConnectionStrings": {
    "DefaultConnection": "Server=localhost;Database=GameConsole;Integrated Security=true;"
  }
}